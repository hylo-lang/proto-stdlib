/// A half-open range from a lower bound up to, but not including, an upper bound.
type Range<Bound: Comparable> {

  invariant("invalid range") lower_bound <= upper_bound

  var lower_bound: Bound

  var upper_bound: Bound

}

extension Range: Copyable where Bound: Copyable {}

extension Range: Equatable {}

extension Range: Hashable where Bound: Hashable {}
